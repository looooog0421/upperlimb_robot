function T = transform(DH)
    
    a = DH(1);
    alpha = DH(2);
    d = DH(3);
    theta = DH(4)

    T = [cos(theta),             -sin(theta),            0,           a;
         sin(theta)*cos(alpha),  cos(theta)*cos(alpha),  -sin(alpha), -d*sin(alpha);
         sin(theta)*sin(alpha)
        
    
    ]



end